{
  "name": "@anansi/router",
  "version": "0.2.6",
  "description": "React 18 optimized router",
  "homepage": "https://github.com/ntucker/anansi/tree/master/packages/router#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:ntucker/anansi.git",
    "directory": "packages/router"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Nathaniel Tucker",
    "email": "me@ntucker.me",
    "url": "https://ntucker.true.io"
  },
  "files": [
    "src",
    "dist",
    "lib",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "module": "lib/index.js",
    "types": "lib/index.d.ts"
  },
  "module": "src/index.ts",
  "types": "src/index.ts",
  "keywords": [
    "router",
    "routing",
    "history",
    "react",
    "browser"
  ],
  "devDependencies": {
    "@anansi/babel-preset": "^2.8.4",
    "@anansi/browserslist-config": "1.3.3",
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.8",
    "jest": "^27.5.1",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "@pojo-router/core": "^0.3.5",
    "history": "5.3.0",
    "nano-memoize": "^1.2.1"
  },
  "peerDependencies": {
    "@types/react": "^17.0.40 || ^18.0.0-0",
    "react": "^18.0.0-0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 12.10.0"
  },
  "browserslist": [
    "extends @anansi/browserslist-config"
  ],
  "scripts": {
    "pretest": "eslint --ext .ts,.tsx .",
    "test": "jest",
    "test:type": "tsc",
    "build": "yarn run build:lib",
    "build:lib": "NODE_ENV=production BROWSERSLIST_ENV='2020' babel --root-mode upward src --out-dir lib --source-maps inline --extensions '.ts,.tsx,.js' --ignore '**/__tests__/**' --ignore '**/*.d.ts'",
    "build:clean": "rimraf lib ts3.4 legacy dist *.tsbuildinfo",
    "prepack": "yarn build"
  }
}
