    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "node": ">= 8.0.0",
