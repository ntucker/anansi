{
  "name": "@anansi/core",
  "version": "0.16.3",
  "description": "React 18 Framework",
  "homepage": "https://github.com/ntucker/anansi/tree/master/packages/core#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:ntucker/anansi.git",
    "directory": "packages/core"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Nathaniel Tucker",
    "email": "me@ntucker.me",
    "url": "https://ntucker.true.io"
  },
  "files": [
    "src",
    "lib",
    "dist",
    "server.d.ts",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "serve-anansi": "./lib/scripts/serve.js",
    "start-anansi": "./lib/scripts/startDevserver.js"
  },
  "sideEffects": false,
  "publishConfig": {
    "module": "lib/index.js",
    "types": "lib/index.d.ts"
  },
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "node": {
        "types": "./lib/index.server.d.ts",
        "require": "./dist/server.js",
        "import": "./lib/index.server.js"
      },
      "types": "./lib/index.d.ts",
      "require": "./dist/client.js",
      "default": "./lib/index.js"
    },
    "./server": {
      "types": "./lib/index.server.d.ts",
      "require": "./dist/server.js",
      "default": "./lib/index.server.js"
    },
    "./scripts": {
      "types": "./lib/scripts/index.d.ts",
      "default": "./lib/scripts/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "ssr",
    "react",
    "suspense",
    "concurrent mode",
    "router",
    "routing",
    "browser"
  ],
  "devDependencies": {
    "@anansi/babel-preset": "workspace:*",
    "@anansi/browserslist-config": "workspace:*",
    "@anansi/webpack-config": "workspace:*",
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@types/compression": "1.7.2",
    "@types/source-map-support": "0.5.6",
    "@types/tmp": "0.2.3",
    "@types/webpack-hot-middleware": "2.25.6",
    "jest": "29.4.1",
    "rimraf": "4.1.2",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  },
  "dependencies": {
    "@anansi/router": "workspace:^",
    "@babel/runtime": "^7.17.0",
    "@rest-hooks/ssr": "^0.7.8",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cross-fetch": "^3.1.5",
    "fs-require": "^1.6.0",
    "history": "^5.3.0",
    "http-proxy-middleware": "^2.0.6",
    "memfs": "^3.4.13",
    "ora": "^5.4.1",
    "redux": "^4.2.1",
    "source-map-support": "^0.5.21",
    "tmp": "^0.2.1",
    "unionfs": "^4.4.0",
    "webpack-dev-server": "^4.11.1",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "@ant-design/cssinjs": "^1.5.1",
    "@rest-hooks/react": "^6.0.0 || ^7.0.0",
    "@rest-hooks/redux": "^6.3.0 || ^7.0.0",
    "@types/react": "^17.0.40 || ^18.0.0-0",
    "@types/react-dom": "^17.0.40 || ^18.0.0-0",
    "react": "^18.0.0-0",
    "react-dom": "^18.0.0-0",
    "webpack": "^5.60.0"
  },
  "peerDependenciesMeta": {
    "@ant-design/cssinjs": {
      "optional": true
    },
    "@rest-hooks/react": {
      "optional": true
    },
    "@rest-hooks/redux": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "engines": {
    "node": "^12.17 || ^13.7 || >=14"
  },
  "scripts": {
    "pretest": "eslint --ext .ts,.tsx .",
    "dev": "yarn run build:lib -w & yarn run build:bundle -w && fg",
    "test": "jest",
    "test:type": "tsc",
    "build": "yarn run build:lib && yarn run build:bundle",
    "build:lib": "NODE_ENV=production BROWSERSLIST_ENV='2020' babel --root-mode upward src --out-dir lib --source-maps inline --extensions '.ts,.tsx,.js' --ignore '**/__tests__/**' --ignore '**/*.d.ts'",
    "build:bundle": "BROWSERSLIST_ENV=node12 webpack --mode=development --target=node",
    "build:clean": "rimraf lib dist ts3.4 legacy *.tsbuildinfo",
    "prepack": "yarn build"
  }
}
