{
  "name": "@anansi/core",
  "version": "0.7.0",
  "description": "React 18 Framework",
  "homepage": "https://github.com/ntucker/anansi/tree/master/packages/core#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:ntucker/anansi.git",
    "directory": "packages/core"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Nathaniel Tucker",
    "email": "me@ntucker.me",
    "url": "https://ntucker.true.io"
  },
  "files": [
    "src",
    "lib",
    "dist",
    "server.d.ts",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "start-anansi": "./lib/scripts/startDevserver.js"
  },
  "sideEffects": false,
  "publishConfig": {
    "module": "lib/index.js",
    "types": "lib/index.d.ts"
  },
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "node": {
        "types": "./lib/index.server.d.ts",
        "require": "./dist/server.js",
        "import": "./lib/index.server.js"
      },
      "types": "./lib/index.d.ts",
      "require": "./dist/client.js",
      "default": "./lib/index.js"
    },
    "./server": {
      "types": "./lib/index.server.d.ts",
      "require": "./dist/server.js",
      "default": "./lib/index.server.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "ssr",
    "react",
    "suspense",
    "concurrent mode",
    "router",
    "routing",
    "browser"
  ],
  "devDependencies": {
    "@anansi/babel-preset": "^3.2.2",
    "@anansi/browserslist-config": "1.3.3",
    "@anansi/webpack-config": "^11.6.0",
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.2",
    "@types/source-map-support": "^0.5.4",
    "@types/tmp": "^0.2.3",
    "@types/webpack-hot-middleware": "^2.25.6",
    "jest": "28.1.0",
    "rimraf": "^3.0.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@anansi/router": "^0.5.4",
    "@babel/runtime": "^7.10.5",
    "@rest-hooks/ssr": "^0.1.3",
    "chalk": "^4.0.0",
    "cross-fetch": "^3.1.5",
    "fs-monkey": "^1.0.3",
    "history": "^5.3.0",
    "import-fresh": "^3.3.0",
    "memfs": "^3.4.1",
    "ora": "^5.0.0",
    "redux": "^4.1.2",
    "source-map-support": "^0.5.21",
    "tmp": "^0.2.1",
    "unionfs": "^4.4.0",
    "webpack-dev-server": "^4.9.0",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "@rest-hooks/core": "^3.0.0",
    "@types/react": "^17.0.40 || ^18.0.0-0",
    "@types/react-dom": "^17.0.40 || ^18.0.0-0",
    "react": "^18.0.0-0",
    "react-dom": "^18.0.0-0",
    "webpack": "^5.60.0"
  },
  "peerDependenciesMeta": {
    "@rest-hooks/core": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 12.10.0"
  },
  "browserslist": [
    "extends @anansi/browserslist-config"
  ],
  "scripts": {
    "pretest": "eslint --ext .ts,.tsx .",
    "dev": "yarn run build:lib -w & yarn run build:bundle -w && fg",
    "test": "jest",
    "test:type": "tsc",
    "build": "yarn run build:lib && yarn run build:bundle",
    "build:lib": "NODE_ENV=production BROWSERSLIST_ENV='2020' babel --root-mode upward src --out-dir lib --source-maps inline --extensions '.ts,.tsx,.js' --ignore '**/__tests__/**' --ignore '**/*.d.ts'",
    "build:bundle": "BROWSERSLIST_ENV=node12 webpack --mode=development --target=node",
    "build:clean": "rimraf lib dist ts3.4 legacy *.tsbuildinfo",
    "prepack": "yarn build"
  }
}
