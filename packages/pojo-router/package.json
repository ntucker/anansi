    "@babel/core": "7.17.5",
    "jest": "^27.5.1",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "history": "^5.2.0",
    "path-to-regexp": "^6.2.0"
  },
  "peerDependencies": {
    "@types/react": "^16.8.0 || ^17.0.0 || ^18.0.0-0",
