    "@anansi/webpack-config": "^14.3.2",
    "@babel/cli": "7.21.0",
    "@babel/core": "7.21.0",
    "jest": "29.4.3",
    "rimraf": "4.1.2",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  },
  "dependencies": {
