{
  "name": "@anansi/polyfill",
  "version": "0.3.37",
  "description": "Feature detection triggered dynamic polyfills",
  "sideEffects": false,
  "keywords": [
    "anansi",
    "polyfill"
  ],
  "author": "Nathaniel Tucker",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/ntucker/anansi/tree/master/packages/polyfill",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ntucker/anansi.git"
  },
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build:commonjs": "rollup -c",
    "build:lib": "NODE_ENV=production babel src --out-dir lib --source-maps inline --extensions '.ts'",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run build:types && npm run build:lib && npm run build:commonjs",
    "dev": "yarn build:lib -w",
    "clean": "rimraf dist lib",
    "prepare": "yarn run clean && yarn run build",
    "lint": "eslint src/*.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "files": [
    "dist",
    "lib"
  ],
  "dependencies": {
    "@types/intl": "^1.2.0",
    "core-js": "^3.0.0",
    "intl": "^1.2.5",
    "ric-shim": "^1.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@anansi/babel-preset": "^0.19.3",
    "@anansi/eslint-plugin": "^0.7.0-beta.2",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "eslint": "^6.7.2",
    "rollup": "^1.27.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "typescript": "^3.7.2"
  },
  "bugs": {
    "url": "https://github.com/ntucker/anansi/issues"
  },
  "gitHead": "150aeb90c96b773030da997a5e9eb1cc83709aa6"
}
