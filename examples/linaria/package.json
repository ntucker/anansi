{
  "name": "anansi-example-linaria",
  "version": "2.0.4",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode=development",
    "prod": "serve ./dist",
    "build": "webpack --mode=production",
    "build:server": "webpack --mode=production --target=node",
    "build:clean": "rm -rf dist",
    "analyze": "webpack --mode=production --env analyze",
    "profile": "webpack --mode=production --env profile",
    "pkgcheck": "webpack --env check=nobuild",
    "type-check": "tsc --noEmit",
    "format": "eslint --fix \"src/**/*.{js,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@anansi/babel-preset": "^2.1.2",
    "@anansi/browserslist-config": "^1.0.1",
    "@anansi/eslint-plugin": "^0.9.15",
    "@anansi/webpack-config": "^5.1.2",
    "@babel/core": "7.12.3",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.3",
    "babel-loader": "8.1.0",
    "react-refresh": "0.9.0",
    "serve": "^11.3.2"
  },
  "dependencies": {
    "@anansi/polyfill": "^1.0.16",
    "@babel/runtime": "^7.12.1",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "linaria": "^2.0.0-rc.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "redbox-react": "^1.6.0"
