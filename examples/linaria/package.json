{
  "name": "example-linaria",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode=development",
    "start:prod": "serve ./dist",
    "build": "webpack --mode=production",
    "build:server": "webpack --mode=production --target=node",
    "build:clean": "rm -rf dist && rm -rf server_assets",
    "build:analyze": "webpack --mode=production --env analyze",
    "build:profile": "webpack --mode=production --env profile",
    "pkgcheck": "webpack --env check=nobuild",
    "test:type": "tsc --noEmit",
    "format": "eslint --fix \"src/**/*.{js,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\""
  },
  "devDependencies": {
    "@anansi/babel-preset": "^2.7.20",
    "@anansi/browserslist-config": "^1.3.3",
    "@anansi/eslint-plugin": "^0.12.2",
    "@anansi/webpack-config": "^10.0.0",
    "@types/classnames": "^2.2.9",
    "@types/react-router-dom": "^5.3.2",
    "babel-loader": "8.2.3",
    "react-refresh": "^0.11.0",
    "serve": "^13.0.2"
  },
  "dependencies": {
    "@anansi/polyfill": "^1.0.89",
    "@babel/plugin-transform-modules-commonjs": "^7.16.5",
    "@babel/runtime": "^7.16.7",
    "@linaria/core": "^3.0.0-beta.15",
    "@linaria/react": "^3.0.0-beta.17",
    "@linaria/shaker": "^3.0.0-beta.17",
    "classnames": "^2.2.6",
    "core-js": "^3.20.2",
    "redbox-react": "^1.6.0"
  },
  "browserslist": [
    "extends @anansi/browserslist-config"
  ]
}
