{
  "name": "anansi-example-linaria",
  "version": "2.2.9-rc.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode=development",
    "prod": "serve ./dist",
    "build": "webpack --mode=production",
    "build:server": "webpack --mode=production --target=node",
    "build:clean": "rm -rf dist",
    "analyze": "webpack --mode=production --env analyze",
    "profile": "webpack --mode=production --env profile",
    "pkgcheck": "webpack --env check=nobuild",
    "type-check": "tsc --noEmit",
    "format": "eslint --fix \"src/**/*.{js,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@anansi/babel-preset": "^2.2.1",
    "@anansi/browserslist-config": "^1.2.0",
    "@anansi/eslint-plugin": "^0.9.24",
    "@anansi/webpack-config": "^6.0.0-rc.0",
    "@linaria/babel-preset": "^3.0.0-beta.1",
    "@linaria/core": "^3.0.0-beta.1",
    "@linaria/react": "^3.0.0-beta.1",
    "@linaria/shaker": "^3.0.0-beta.1",
    "@linaria/webpack-loader": "^3.0.0-beta.1",
    "@types/classnames": "^2.2.9",
    "@types/react-router-dom": "^5.1.3",
    "babel-loader": "8.2.2",
    "react-refresh": "0.9.0",
    "serve": "^11.3.2"
  },
  "dependencies": {
    "@anansi/polyfill": "^1.0.29",
    "@babel/runtime": "^7.12.13",
    "classnames": "^2.2.6",
    "core-js": "^3.8.3",
    "linaria": "^3.0.0-beta.1",
    "redbox-react": "^1.6.0"
  },
  "browserslist": [
    "extends @anansi/browserslist-config"
  ]
