    "@types/react-router-dom": "5.3.3",
    "babel-loader": "9.1.0",
    "react-refresh": "0.14.0",
    "serve": "14.1.2",
    "webpack": "5.75.0"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "7.19.6",
    "@babel/runtime": "^7.17.2",
    "@linaria/core": "4.2.3",
    "@linaria/react": "4.3.1",
    "@linaria/shaker": "4.2.5",
