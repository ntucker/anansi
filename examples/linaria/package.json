{
  "name": "anansi-example-linaria",
  "version": "2.4.2",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode=development",
    "start:prod": "serve ./dist",
    "build": "webpack --mode=production",
    "build:server": "webpack --mode=production --target=node",
    "build:clean": "rm -rf dist && rm -rf server_assets",
    "build:analyze": "webpack --mode=production --env analyze",
    "build:profile": "webpack --mode=production --env profile",
    "pkgcheck": "webpack --env check=nobuild",
    "test:type": "tsc --noEmit",
    "format": "eslint --fix \"src/**/*.{js,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\""
  },
  "devDependencies": {
    "@anansi/babel-preset": "^2.7.1",
    "@anansi/browserslist-config": "^1.3.0",
    "@anansi/eslint-plugin": "^0.11.13",
    "@anansi/webpack-config": "^7.2.2",
    "@types/classnames": "^2.2.9",
    "@types/react-router-dom": "^5.1.3",
    "babel-loader": "8.2.2",
    "react-refresh": "^0.11.0-alpha-01be61c12",
    "serve": "^12.0.0"
  },
  "dependencies": {
    "@anansi/polyfill": "^1.0.65",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/runtime": "^7.14.5",
    "@linaria/core": "^3.0.0-beta.4",
    "@linaria/react": "^3.0.0-beta.4",
    "@linaria/shaker": "^3.0.0-beta.5",
    "classnames": "^2.2.6",
    "core-js": "^3.14.0",
    "redbox-react": "^1.6.0"
  },
  "browserslist": [
    "extends @anansi/browserslist-config"
  ]
}
