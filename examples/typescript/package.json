{
  "name": "anansi-example-typescript",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode=development",
    "prod": "serve ./dist",
    "build": "webpack --mode=production",
    "build:server": "webpack --mode=production --target=node",
    "build:clean": "rm -rf dist",
    "analyze": "webpack --mode=production --analyze",
    "profile": "webpack --mode=production --profile",
    "pkgcheck": "webpack --check=nobuild",
    "type-check": "tsc --noEmit",
    "format": "eslint --fix \"src/**/*.{js,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@anansi/babel-preset": "^1.2.4",
    "@anansi/browserslist-config": "^1.0.0",
    "@anansi/eslint-plugin": "^0.9.7",
    "@anansi/webpack-config": "^1.0.6",
    "@babel/core": "7.10.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/storybook__react": "^5.2.1",
    "babel-loader": "8.1.0",
    "eslint": "^7.1.0",
    "react-refresh": "0.8.3",
    "serve": "^11.3.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@anansi/polyfill": "^1.0.7",
    "@babel/runtime": "^7.10.1",
    "antd": "^4.3.5",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "lodash": "^4.17.15",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "^5.2.0",
    "react-router-scroll-4": "^1.0.0-beta.2",
    "redbox-react": "^1.6.0",
    "rest-hooks": "5.0.0-gamma.0"
  },
  "browserslist": [
    "extends @anansi/browserslist-config"
  ]
}
