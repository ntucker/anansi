{
  "name": "example-react-18",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode=development",
    "start:prod": "serve ./dist",
    "build": "webpack --mode=production",
    "build:server": "webpack --mode=production --target=node",
    "build:clean": "rm -rf dist && rm -rf server_assets",
    "build:analyze": "webpack --mode=production --env analyze",
    "build:profile": "webpack --mode=production --env profile",
    "pkgcheck": "webpack --env check=nobuild",
    "test:type": "tsc --noEmit",
    "format": "eslint --fix \"src/**/*.{js,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\""
  },
  "devDependencies": {
    "@anansi/babel-preset": "^2.8.4",
    "@anansi/browserslist-config": "^1.3.3",
    "@anansi/eslint-plugin": "^0.12.9",
    "@anansi/jest-preset": "^0.6.2",
    "@anansi/webpack-config": "^11.3.0",
    "@rest-hooks/test": "7.2.1",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "jest": "^27.5.1",
    "react-refresh": "^0.11.0",
    "react-test-renderer": "18.0.0-rc.0",
    "serve": "^13.0.2",
    "webpack": "5.69.1"
  },
  "dependencies": {
    "@anansi/router": "^0.2.0",
