{
  "name": "example-react-18",
  "version": "1.1.7",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode=development",
    "start:devserver": "babel-node --extensions '.ts,.tsx,.js' ./scripts/startDevserver.ts ./src/index.tsx",
    "start:prod": "serve ./dist",
    "start:server": "node ./dist-server/main.js",
    "build": "webpack --mode=production",
    "build:server": "webpack --mode=production --target=node",
    "build:clean": "rm -rf dist && rm -rf server_assets",
    "build:analyze": "webpack --mode=production --env analyze",
    "build:profile": "webpack --mode=production --env profile",
    "pkgcheck": "webpack --env check=nobuild",
    "test:type": "tsc --noEmit",
    "format": "eslint --fix \"src/**/*.{js,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\""
  },
  "devDependencies": {
    "@anansi/babel-preset": "^3.0.3",
    "@anansi/browserslist-config": "^1.3.3",
    "@anansi/eslint-plugin": "^0.12.10",
    "@anansi/jest-preset": "^0.6.7",
    "@anansi/webpack-config": "^11.3.9",
    "@babel/node": "^7.16.8",
    "@rest-hooks/test": "7.3.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "27.4.1",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "jest": "^27.5.1",
    "react-refresh": "0.12.0",
    "react-test-renderer": "18.0.0",
    "serve": "^13.0.2",
    "webpack": "5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "4.8.1",
    "webpack-hot-middleware": "^2.25.1"
  },
  "dependencies": {
    "@anansi/router": "^0.3.4",
    "@babel/runtime": "^7.17.2",
    "@linaria/core": "^3.0.0-beta.18",
    "@linaria/react": "3.0.0-beta.18",
    "@linaria/shaker": "3.0.0-beta.18",
    "@rest-hooks/img": "0.6.0",
    "@rest-hooks/rest": "5.0.0",
    "@types/node": "17.0.24",
    "antd": "4.19.5",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "core-js": "3.22.0",
    "cross-fetch": "^3.1.5",
    "express": "^4.17.3",
    "fs-monkey": "^1.0.3",
    "history": "5.3.0",
    "memfs": "^3.4.1",
    "ora": "^5.0.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "redux": "^4.1.2",
    "rest-hooks": "6.3.1",
    "source-map-support": "^0.5.21",
    "tmp": "^0.2.1",
    "unionfs": "^4.4.0",
    "whatwg-fetch": "^3.6.2"
  },
  "browserslist": [
    "extends @anansi/browserslist-config"
  ]
}
