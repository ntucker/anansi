    "@rest-hooks/img": "^0.5.2",
    "@rest-hooks/rest": "3.0.2",
    "@types/node": "17.0.18",
    "antd": "4.18.7",
    "classnames": "^2.2.6",
    "core-js": "3.21.1",
    "history": "^5.2.0",
    "nano-memoize": "^1.2.1",
    "react": "18.0.0-rc.0",
    "react-dom": "18.0.0-rc.0",
    "rest-hooks": "6.2.0-beta.3"
  },
  "browserslist": [
    "extends @anansi/browserslist-config"
