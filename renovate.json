{
  "extends": ["group:allNonMajor", "group:definitelyTyped"],
  "packageRules": [
    {
      "groupName": "storybook",
      "matchPackagePrefixes": [
        "@storybook/"
      ]
    },
    {
      "groupName": "babel",
      "matchPackagePrefixes": [
        "@babel/"
      ]
    },
    {
      "groupName": "linaria",
      "matchPackagePrefixes": [
        "@linaria/"
      ]
    },
    {
      "groupName": "rest hooks",
      "matchPackagePrefixes": [
        "@rest-hooks/"
      ]
    }
  ],
	"assigneesFromCodeOwners": true,
	"commitMessagePrefix": "pkg: ",
	"commitMessageTopic": "`{{depName}}`",
	"dependencyDashboardApproval": true,
	"rangeStrategy": "auto",
  "ignoreDeps": ["whatwg-fetch","react","react-dom","react-test-renderer","react-refresh", "react-error-overlay"],
  "timezone": "America/Chicago",
  "schedule": ["after 6pm and before 1am every weekday", "every weekend"]
}
